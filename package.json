{
  "name": "ractive-realworld-example-app",
  "version": "0.2.0",
  "description": "> ### RactiveJS codebase containing real world examples (CRUD, auth, advanced patterns, etc) that adheres to the [RealWorld](https://github.com/gothinkster/realworld) spec and API.",
  "main": "server.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "parse": "node ./tools/template-parser.js",
    "debug": "npm run parse && env NODE_ENV=development ./node_modules/.bin/webpack --display-error-details",
    "build": "npm run parse && env NODE_ENV=production ./node_modules/.bin/webpack --display-error-details",
    "dev": "node server.js",
    "start": "env NODE_ENV=production node server.js"
  },
  "config": {
    "port": "8080"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PaulMaly/realworld-starter-kit.git"
  },
  "author": "Paul Maly",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PaulMaly/realworld-starter-kit/issues"
  },
  "homepage": "https://github.com/PaulMaly/realworld-starter-kit#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "compression": "^1.7.1",
    "consolidate": "^0.15.0",
    "express": "^4.16.2",
    "express-http-proxy": "^1.1.0",
    "express-session": "^1.15.6",
    "helmet": "^3.9.0",
    "method-override": "^2.3.10",
    "mustache": "^2.3.0",
    "ractive": "edge",
    "ractive-page": "https://github.com/ractivejs-developers/ractive-page.git",
    "ractive-ready": "https://github.com/ractivejs-developers/ractive-ready.git",
    "ractive-transitions-fade": "^0.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "compression-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^4.2.1",
    "expose-loader": "^0.7.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "glob": "^7.1.2",
    "html-webpack-plugin": "^2.30.1",
    "uglify-js": "^3.1.10",
    "uglifyjs-webpack-plugin": "^1.1.4",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "2.9.1",
    "webpack-dev-server": "^2.9.5",
    "webpack-manifest-plugin": "^1.3.2",
    "webpack-md5-hash": "^0.0.5",
    "webpack-runtime-analyzer": "^1.5.0",
    "wrapper-webpack-plugin": "^1.0.0"
  }
}
